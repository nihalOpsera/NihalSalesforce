    <fields>
        <fullName>APTS_ConfigureProductsTP__c</fullName>
        <externalId>false</externalId>
        <formula>IF(LEN(Apttus_QPConfig__PriceListId__c) &gt; 0,IF(NokiaCPQ_Environment_Access_Type__c = &apos;sCRM&apos;,IF((Quote_Type__c == &apos;Direct CPQ&apos; &amp;&amp; (Is_List_Price_Only__c=FALSE &amp;&amp; NOT(ISPICKVAL(NokiaCPQ_Portfolio__c ,&apos;Fixed Network&apos;)))),(&quot;/apex/Apttus_Lightning_Page_Redirect?id=&quot; &amp;Id &amp; &quot;&amp;flow=DirectFlow&quot; &amp; &quot;&amp;cntrNbr_1=&quot;&amp;Direct_Price_List_Number__c),IF(Quote_Type__c == &apos;Indirect CPQ&apos;,(&quot;/apex/Apttus_Lightning_Page_Redirect?id=&quot; &amp;Id &amp; &quot;&amp;flow=IndirectFlow&quot; &amp; &quot;&amp;cntrNbr_1=&quot;&amp;Project_Price_List_Number__c),IF((Is_List_Price_Only__c=FALSE &amp;&amp; (ISPICKVAL(NokiaCPQ_Portfolio__c , &apos;Fixed Network&apos;))),(&quot;/apex/Apttus_Lightning_Page_Redirect?id=&quot; &amp;Id &amp; &quot;&amp;flow=Turbo Flow for FN&quot; &amp; &quot;&amp;cntrNbr_1=&quot;&amp;Direct_Price_List_Number__c),null))),null),null)</formula>
        <label>Configure Product TP</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>APTS_Configure_Products_TP__c</fullName>
        <externalId>false</externalId>
        <formula>IF ( AND(LEN( Apttus_QPConfig__PriceListId__c ) &gt; 0,  CONTAINS(Quote_Type__c, &apos;CPQ&apos;) ) , HYPERLINK(&quot;/apex/Apttus_Lightning_Page_Redirect?id=&quot; &amp;Id &amp; &quot;&amp;flow=&quot; &amp; IF(Quote_Type__c == &apos;Direct CPQ&apos;, &apos;DirectFlow&amp;cntrNbr_1=&apos; &amp; Direct_Price_List_Number__c, &apos;IndirectFlow&amp;cntrNbr_1=&apos; &amp; Project_Price_List_Number__c), IMAGE(&quot;/resource/Button_Configure_Custom&quot;, &quot;Configure Products/Pricing&quot;),&quot;_self&quot;), NULL)</formula>
