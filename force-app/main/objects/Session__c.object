    <validationRules>
        <fullName>End_date_smaller_than_start_date</fullName>
        <active>true</active>
        <description>End Date CanÂ´t be smaller than Start Date
Time shouldn&apos;t be blank if correspondent Date is</description>
        <errorConditionFormula>OR(
				  AND(  ISBLANK(Start_time__c), NOT(ISBLANK(Start_hour__c))
									)
				, AND(  ISBLANK(End_time__c),   NOT(ISBLANK(End_hour__c))
									)
				, AND (  End_time__c = Start_time__c
								         , End_hour__c &lt;= Start_hour__c
								       )
		       
   )</errorConditionFormula>
        <errorMessage>Please, fill the Date fields properly:
- End Date/Time cannot be smaller than Start Date/Time;
- Time shouldn&apos;t be blank if correspondent Date is.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Session_date_check</fullName>
        <active>true</active>
        <errorConditionFormula>OR (
ISBLANK(Event__r.Event_End_Date__c) ,
ISBLANK(Event__r.Event_Start_Date__c) ,
End_time__c &gt;  Event__r.Event_End_Date__c ,
Start_time__c  &lt; Event__r.Event_Start_Date__c)</errorConditionFormula>
        <errorMessage>The date of the session should fallen between the dates of the event. Please check the event&apos;s dates.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_and_End_Date</fullName>
        <active>true</active>
        <description>Start and End Date of the Session should be the same</description>
        <errorConditionFormula>Start_time__c !=  End_time__c</errorConditionFormula>
        <errorMessage>The session should have the same start and end dates.</errorMessage>
    </validationRules>
