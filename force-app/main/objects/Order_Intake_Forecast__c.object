    <fields>
        <fullName>Competitor_Bypass__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
AND((MINUTE(TIMEVALUE(NOW()))-MINUTE(TIMEVALUE(CreatedDate))&lt;1),
OR(
Opportunity__r.Is_Auto_Upsell__c=True,
!ISPICKVAL( Opportunity__r.Lead_Type__c ,&apos;&apos;)


) ) ,True,

IF(
AND(
 
!ISPICKVAL( Opportunity__r.Sales_Outcome__c ,&apos;Won&apos;),
!ISPICKVAL( Opportunity__r.Sales_Outcome__c ,&apos;Won (Requested)&apos;),
!ISPICKVAL( Opportunity__r.Sales_Outcome__c ,&apos;&apos;)
) 
,True,
IF(
AND(
OR(
Opportunity__r.Previous_Sales_Outcome__c&lt;&gt;&apos;Won&apos;,
Opportunity__r.Previous_Sales_Outcome__c&lt;&gt;&apos;Won (Requested)&apos;
),

Update_Status__c=true


),True,False

)


)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Competitor Bypass</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DQI_PREDICTIVE__c</fullName>
        <description>Req#2655 : Calculates Data Quality OIF Value EUR</description>
        <externalId>false</externalId>
        <formula>if(OIF_Value_EUR__c &lt;&gt; 0, Data_Quality_OIF_Value_EUR_PREDICTIVE__c / OIF_Value_EUR__c, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
