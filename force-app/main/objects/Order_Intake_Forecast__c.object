        <fullName>Data_Quality_Info_PREDICTIVE__c</fullName>
        <description>Req #3744 : Shows Data Quality Predictive Issues</description>
        <externalId>false</externalId>
        <formula>IF(Opportunity__r.Data_Quality_Plan_Date_PREDICTIVE__c, 
   IF(Data_Quality_Past_Due_PREDICTIVE__c, 
     IF(OR((TODAY()-(DATEVALUE(Opportunity__r.Last_User_Modified__c)))&gt;90, 
(DATEVALUE($Label.Current_Period_End_Date) - DATEVALUE(Opportunity__r.Last_User_Modified__c)) &gt;= 90),&apos;All 3 DQI Predictive Issues&apos;, &apos;Past Gate &amp; Past Due Predictive&apos;), 
    IF(OR((TODAY()-(DATEVALUE(Opportunity__r.Last_User_Modified__c)))&gt;90, 
(DATEVALUE($Label.Current_Period_End_Date) - DATEVALUE(Opportunity__r.Last_User_Modified__c)) &gt;= 90),&apos;Past Gate &amp; Last Modified Predictive&apos;,&apos;Past Gate Predictive Only&apos;)
	),
	IF(Data_Quality_Past_Due_PREDICTIVE__c,
	  IF(OR((TODAY()-(DATEVALUE(Opportunity__r.Last_User_Modified__c)))&gt;90, 
(DATEVALUE($Label.Current_Period_End_Date) - DATEVALUE(Opportunity__r.Last_User_Modified__c)) &gt;= 90),&apos;Past Due &amp; Last Modified Predictive&apos;,&apos;Past Due Predictive Only&apos;),
	  IF(OR((TODAY()-(DATEVALUE(Opportunity__r.Last_User_Modified__c)))&gt;90, 
(DATEVALUE($Label.Current_Period_End_Date) - DATEVALUE(Opportunity__r.Last_User_Modified__c)) &gt;= 90),&apos;Last Modified Predictive Only&apos;,&apos;No DQ Predictive Issues&apos;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Data Quality Info PREDICTIVE</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Data_Quality_Info__c</fullName>
        <externalId>false</externalId>
        <formula>BLANKVALUE(If(Opportunity__r.DQ_Plan_Date__c, 
	If(Past_Due__c, 
		IF(DQ_LastModifiedDate__c,&apos;All 3 DQIs&apos;, 
			IF(!DQ_LastModifiedDate__c,&apos;Past Due &amp; Past Gate&apos;,&apos;&apos;)),
				IF(DQ_LastModifiedDate__c,&apos;Past Gate &amp; Last Modified&apos;,
					IF(!DQ_LastModifiedDate__c,&apos;Past Gate Only&apos;,&apos;&apos;))),
						IF(Past_Due__c, IF(!DQ_LastModifiedDate__c,&apos;Past Due Only&apos;,
							IF(DQ_LastModifiedDate__c,&apos;Past Due &amp; Last Modified&apos;,&apos;&apos;)),
								IF(DQ_LastModifiedDate__c,&apos;Last Modified Only&apos;,&apos;&apos;))),&apos;No DQ Issue&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Data Quality Info</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
