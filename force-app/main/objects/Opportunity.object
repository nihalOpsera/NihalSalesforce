ISCHANGED(StageName ) &amp;&amp; 
ISPICKVAL(StageName ,&apos;Handover (Prepare for Delivery)&apos;) &amp;&amp;
$Profile.Name  &lt;&gt; &apos;System Administrator&apos;  &amp;&amp; 
