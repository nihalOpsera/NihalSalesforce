$Profile.Name &lt;&gt; $Label.Data_Loader_Profile_Name ,
$Profile.Name &lt;&gt; $Label.Admin,
$Profile.Name &lt;&gt; $Label.Nokia_Admin,
$Profile.Name &lt;&gt; $Label.Global_Admin,
 Upsell_Change_Accepted__c = false,
 Upsell_Change_Approved_by__c &lt;&gt; null
)</errorConditionFormula>
