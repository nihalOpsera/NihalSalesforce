            <errorMessage>Value does not exist or does not match filter criteria.</errorMessage>
            <filterItems>
                <field>Opportunity.Business_Type__c</field>
                <operation>equals</operation>
                <value>Committed Upsell</value>
            </filterItems>
            <filterItems>
                <field>$Profile.Name</field>
                <operation>equals</operation>
                <value>Data Loader Profile</value>
            </filterItems>
            <filterItems>
                <field>Opportunity.Phase_Status__c</field>
