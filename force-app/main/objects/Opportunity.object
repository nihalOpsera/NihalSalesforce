                <value>Submitted for G2 Approval, Submitted for G3 Approval, Submitted for G4 Approval, Submitted for Gate 5 Contract Approval, Submitted for G6 Approval, Pending Win/Loss Declaration</value>
            </filterItems>
            <filterItems>
                <field>Opportunity.RecordType.DeveloperName</field>
                <operation>equals</operation>
                <value>Direct_Record_Type,Indirect_Record_Type</value>
            </filterItems>
            <infoMessage>Please enter &apos;Committed Upsell&apos; Opportunity</infoMessage>
            <isOptional>false</isOptional>
        </lookupFilter>
