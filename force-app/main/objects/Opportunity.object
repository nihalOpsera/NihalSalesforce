    </validationRules>
    <validationRules>
        <fullName>Prevent_Close_at_Won</fullName>
        <active>true</active>
        <description>Req-3254: Will prevent User to Close Opportunity via Sales Path when Won</description>
        <errorConditionFormula>AND(
OR(
RecordType.DeveloperName == &apos;Direct_Record_Type&apos;, 
RecordType.DeveloperName == &apos;Indirect_Record_Type&apos;),
 OR(
ISPICKVAL(PRIORVALUE(StageName),&quot;Handover (Prepare for Delivery)&quot;),
ISPICKVAL(PRIORVALUE(StageName),&quot;Execute (Start Delivery)&quot;),
ISPICKVAL(PRIORVALUE(StageName),&quot;Completed&quot;)),
 OR(
ISPICKVAL(StageName,&quot;Closed - Lost to Competitor&quot;),
ISPICKVAL(StageName,&quot;Closed - Withdrawn by Nokia&quot;),
ISPICKVAL(StageName,&quot;Closed - Cancelled by Customer&quot;),
ISPICKVAL(StageName,&quot;Closed - Obsolete&quot;)
), 
$Profile.Name&lt;&gt;$Label.Data_Loader_Profile_Name
)</errorConditionFormula>
        <errorMessage>Cannot close Opportunity when it is Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_FOC_Creation</fullName>
        <active>true</active>
        <description>Pre-Sales &gt; Feb 18&gt; Sprint 19 &gt; Req#5560</description>
        <errorConditionFormula>AND ($Profile.Name &lt;&gt; $Label.Data_Loader_Profile_Name, 
ISNEW(),
ISPICKVAL(PSR_Pre_Sales_Risk_Opportunity_Type__c, &quot;Free of Charge&quot;))</errorConditionFormula>
        <errorMessage>You cannot use this type of Opportunity. During Sales Phase Free of Charge is replaced by Trial and Demo governance. For Execute Phase Free of Charge is covered by the value leakage process</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PricingSupportDecline_Reason_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND ($Profile.Name != &apos;Data Loader Profile&apos;, 
ISPICKVAL(PRMPricing_Support_Status__c, &apos;Declined&apos;), ISPICKVAL(Pricing_Support_Declined_Reason__c, &apos;&apos;))</errorConditionFormula>
        <errorDisplayField>Pricing_Support_Declined_Reason__c</errorDisplayField>
        <errorMessage>You have declined the Pricing Support. Please provide the Declined Reason for the Technical Support.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Primary_Competitor_count</fullName>
        <active>true</active>
        <errorConditionFormula>Count_primary_competitors__c &gt;1</errorConditionFormula>
        <errorMessage>You can&apos;t have more than one Primary competitor.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Probability_Cant_Be_Zero</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT( ISNEW() ) , 
OR( 
RecordType.DeveloperName == &apos;Direct_Record_Type&apos;, 
RecordType.DeveloperName == &apos;Indirect_Record_Type&apos; 
),
ISCHANGED( Probability ) ,
Probability = 0,
OR(
ISPICKVAL(StageName, &apos;Identify Opportunity&apos;),
ISPICKVAL( StageName , &quot;Develop Opportunity&quot;), 
ISPICKVAL( StageName , &quot;Create Offer (Bid)&quot;), 
ISPICKVAL( StageName , &quot;Win the Case (Negotiate)&quot;) 
),
!ISCHANGED( StageName ) ,
!ISPICKVAL(Apttus_Approval__Approval_Status__c , &apos;Pending Approval&apos;), 
$Profile.Name&lt;&gt;$Label.Data_Loader_Profile_Name 
)</errorConditionFormula>
        <errorMessage>Probability should be between 1 and 100.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Record_Type_Change_Direct_to_Indirect</fullName>
        <active>true</active>
        <description>Req #3287 Opportunity record type change from direct to indirect</description>
        <errorConditionFormula>AND(
NF_Facilitator_Notified__c = true	,
ISCHANGED( RecordTypeId ), 
OR(
RecordTypeId = $Label.Indirect_Opportunity, 
RecordTypeId = $Label.Direct_Opportunity
) , 
OR(
PRIORVALUE(RecordTypeId )= $Label.Indirect_Opportunity,
PRIORVALUE(RecordTypeId )= $Label.Direct_Opportunity
), 
NOT( 
OR( 
$Profile.Name = &apos;System Administrator&apos;, 
$Profile.Name = &apos;Data Loader Profile&apos;, 
$Profile.Name = &apos;Nokia Admin Profile&apos;, 
$Profile.Name = &apos;Global Process Admin Profile&apos;	
) 
) 
)</errorConditionFormula>
        <errorMessage>You are not allowed to change the Opportunity record type after the G3 Facilitator is Notified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RestrictPreSalesOpptyFromDirectIndirect</fullName>
        <active>true</active>
        <description>D-2415: Requirement 2522:</description>
        <errorConditionFormula>AND( 
OR( 
ISNEW(), 
ISCHANGED(Reference_Opportunity__c) 
), 
OR( 
RecordType.DeveloperName == &apos;Direct_Record_Type&apos;, 
RecordType.DeveloperName == &apos;Indirect_Record_Type&apos; 
), 
OR( 
Reference_Opportunity__r.RecordType.DeveloperName == &apos;PSR_Risk_Order_Risk_Shipment&apos;, 
Reference_Opportunity__r.RecordType.DeveloperName == &apos;Read_Only_Pre_Sales_Risk_Opportunity&apos; 
), 
