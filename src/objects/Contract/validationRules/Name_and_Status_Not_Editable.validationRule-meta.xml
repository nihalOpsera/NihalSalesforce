<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Name_and_Status_Not_Editable</fullName>
    <active>true</active>
    <errorConditionFormula>AND(OR( ISCHANGED(AccountId), ISCHANGED(Status)),  $Profile.Name = &apos;Non-Sales User Profile&apos;)</errorConditionFormula>
    <errorMessage>Account Name and Status is not editable.</errorMessage>
</ValidationRule>
