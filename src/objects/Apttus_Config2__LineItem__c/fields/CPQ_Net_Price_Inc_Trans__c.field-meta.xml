<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CPQ_Net_Price_Inc_Trans__c</fullName>
    <description>Calculated as Net Price + Transport Price</description>
    <externalId>false</externalId>
    <formula>IF(TEXT(Apttus_Config2__LineType__c) = &apos;Option&apos; , null, IF(CPQ_Price_Class__c = &apos;Transportation&apos;,Apttus_Config2__BasePriceOverride__c, Apttus_Config2__NetPrice__c + CPQ_Transport_Price__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculated as Net Price + Transport Price</inlineHelpText>
    <label>Net Price (Incl Transport)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>5</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
