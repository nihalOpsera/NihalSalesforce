<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AccCurrencyShouldEqualPriceListCurrency</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT( ISBLANK(NokiaCPQ_Contract_Price_List__c) ) , NokiaCPQ_Account_Currency__c &lt;&gt; TEXT(NokiaCPQ_Contract_Price_List__r.CurrencyIsoCode))</errorConditionFormula>
    <errorDisplayField>NokiaCPQ_Contract_Price_List__c</errorDisplayField>
    <errorMessage>No Contract PriceList Exists for this Account</errorMessage>
</ValidationRule>
