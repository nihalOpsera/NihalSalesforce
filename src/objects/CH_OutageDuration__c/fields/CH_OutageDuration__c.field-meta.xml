<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CH_OutageDuration__c</fullName>
    <description>It is used to calculate the difference between duration end date and start date</description>
    <externalId>false</externalId>
    <formula>((CH_DurationEndDate__c - CH_DurationStartDate__c) * 86400 + SECOND(TIMEVALUE(CH_DurationStartDate__c)) - SECOND(TIMEVALUE(CH_DurationEndDate__c)))/60</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Outage Duration (Minutes)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
