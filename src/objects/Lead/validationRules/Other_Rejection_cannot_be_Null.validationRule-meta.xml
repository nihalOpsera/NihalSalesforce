<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Other_Rejection_cannot_be_Null</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL( Distributor_PRM_Rejection_Reason__c , &apos;Other&apos;) , Other_Rejection_Reason_Distributor_PRM__c  = Null)</errorConditionFormula>
    <errorDisplayField>Other_Rejection_Reason_Distributor_PRM__c</errorDisplayField>
    <errorMessage>You have indicated Other in Distributor PRM Rejection Reason field, Please enter details for Other Rejection Reason.</errorMessage>
</ValidationRule>
