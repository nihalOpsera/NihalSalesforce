<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NCP_Entitlement_Relationship__c</fullName>
    <description>Defines if the Entitlement is related to:
- Product (Only related to Product and Product Services in Community)
- Services (Only related to Services Tiles in Community) 
- Product and Services (Related to Product, Product Services and Service Tiles)</description>
    <externalId>false</externalId>
    <label>Entitlement Relationship</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>true</sorted>
            <value>
                <fullName>Product</fullName>
                <default>false</default>
                <label>Product</label>
            </value>
            <value>
                <fullName>Product and Services</fullName>
                <default>true</default>
                <label>Product and Services</label>
            </value>
            <value>
                <fullName>Services</fullName>
                <default>false</default>
                <label>Services</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
