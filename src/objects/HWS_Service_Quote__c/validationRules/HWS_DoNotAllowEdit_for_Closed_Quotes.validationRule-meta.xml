<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HWS_DoNotAllowEdit_for_Closed_Quotes</fullName>
    <active>true</active>
    <errorConditionFormula>($Profile.Name &lt;&gt; &apos;System Administrator&apos;) &amp;&amp; NOT(ISBLANK( PRIORVALUE(HWS_QuoteClosedDate__c) )) &amp;&amp; NOT(ISCHANGED(HWS_Escalated_Date__c))</errorConditionFormula>
    <errorMessage>Not allowed to update when status is Completed or Cancelled.</errorMessage>
</ValidationRule>
