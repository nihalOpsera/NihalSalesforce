<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HWS_Quantity_Can_Not_Be_Negative</fullName>
    <active>true</active>
    <errorConditionFormula>OR(HWS_Quantity__c   &lt;= 0 ,
			ISBLANK( HWS_Quantity__c ))</errorConditionFormula>
    <errorDisplayField>HWS_Quantity__c</errorDisplayField>
    <errorMessage>Quantity can not be less than zero</errorMessage>
</ValidationRule>
