<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CH_PreventFieldsUpdate</fullName>
    <active>true</active>
    <description>NOKIASC-35832</description>
    <errorConditionFormula>AND(NOT(ISNEW()),
OR(ISCHANGED(CH_AccountName__c), ISCHANGED(CH_Product__c)))</errorConditionFormula>
    <errorDisplayField>CH_AccountName__c</errorDisplayField>
    <errorMessage>You are not allowed to update this Field.</errorMessage>
</ValidationRule>
