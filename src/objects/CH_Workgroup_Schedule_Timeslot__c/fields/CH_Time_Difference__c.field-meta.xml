<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CH_Time_Difference__c</fullName>
    <description>Time difference in seconds to GMT time</description>
    <externalId>false</externalId>
    <formula>CASE(TEXT(CH_Workgroup_Schedule__r.CH_Time_Zone__c),&quot;UTC&quot;,0,&quot;UTC -1&quot;,-1,&quot;UTC -2&quot;,-2,&quot;UTC -2:30&quot;,-2.5,&quot;UTC -3&quot;,-3,&quot;UTC -3:30&quot;,-3.5,
					&quot;UTC -4&quot;,-4,&quot;UTC -5&quot;,-5,&quot;UTC -6&quot;,-6,&quot;UTC -7&quot;,-7,&quot;UTC -8&quot;,-8,&quot;UTC -9&quot;,-9,&quot;UTC -9:30&quot;,-9.5,&quot;UTC -10&quot;,-10,&quot;UTC -11&quot;,-11,
					&quot;UTC -12&quot;,-12,&quot;UTC +0&quot;,0,&quot;UTC +1&quot;,1,&quot;UTC +2&quot;,2,&quot;UTC +3&quot;,3,&quot;UTC +3:30&quot;,3.5,&quot;UTC +4&quot;,4,&quot;UTC +4:30&quot;,4.5,&quot;UTC +5&quot;,5,
					&quot;UTC +5:30&quot;,5.5,&quot;UTC +5:45&quot;,5.75,&quot;UTC +6&quot;,6,&quot;UTC +6:30&quot;,6.5,&quot;UTC +7&quot;,7,&quot;UTC +8&quot;,8,&quot;UTC +8:30&quot;,8.5,&quot;UTC +8:45&quot;,8.75,
					&quot;UTC +9&quot;,9,&quot;UTC +9:30&quot;,9.5,&quot;UTC +10&quot;,10,&quot;UTC +11&quot;,11,&quot;UTC +12&quot;,12,&quot;UTC +12:45&quot;,12.75,&quot;UTC +13&quot;,13,&quot;UTC +13:45&quot;,13.75,&quot;UTC +14&quot;,14,
					0)* 60 * 60</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Time Difference</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
