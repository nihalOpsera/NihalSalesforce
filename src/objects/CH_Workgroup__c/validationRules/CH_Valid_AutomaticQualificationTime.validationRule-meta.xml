<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CH_Valid_AutomaticQualificationTime</fullName>
    <active>true</active>
    <errorConditionFormula>OR(CH_AutomaticQualificationTimer__c &lt; 0,CH_AutomaticQualificationTimer__c &gt; 9999)</errorConditionFormula>
    <errorDisplayField>CH_AutomaticQualificationTimer__c</errorDisplayField>
    <errorMessage>Please enter an integer value between 0 and 9999 hours.</errorMessage>
</ValidationRule>
