<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CH_Portal_open_tickets</fullName>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>CASES.TYPE</columns>
    <columns>CASES.SUBJECT</columns>
    <columns>CASES.STATUS</columns>
    <columns>CASES.CREATED_DATE_DATE_ONLY</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>CASES.STATUS</field>
        <operation>notEqual</operation>
        <value>Closed,Completed,Cancelled</value>
    </filters>
    <filters>
        <field>CASES.TYPE</field>
        <operation>notEqual</operation>
    </filters>
    <filters>
        <field>CH_RecordTypeDevName__c</field>
        <operation>equals</operation>
        <value>CH_NokiaStandardCase,CH_NokiaInformationRequest,CH_NokiaIncidentWithRestoration</value>
    </filters>
    <label>All Open SWS Support Tickets - Portal</label>
    <sharedTo>
        <group>All_external_users</group>
    </sharedTo>
</ListView>
