<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CH_EnhancementRequest__c</fullName>
    <description>Modified as a part of NOKIASC-31162.Checking if Internal Status = Feature/Enhance Request then updating the value as YES, else NO</description>
    <externalId>false</externalId>
    <formula>IF( ISPICKVAL(CH_InternalStatus__c, 
	&quot;Feature/Enhancement Request&quot;),
	  &quot;Yes&quot;,
   &quot;No&quot; )</formula>
    <label>Enhancement Request</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
