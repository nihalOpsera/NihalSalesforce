<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CH_InitialResponseDate1</fullName>
    <active>true</active>
    <errorConditionFormula>AND(CH_RecordTypeCheck__c ,
CH_InitialResponse__c  &gt; CH_SolutionProvided__c, 
CH_TemporarySolutionNotApplicable__c )</errorConditionFormula>
    <errorMessage>Initial Response Date should be less than or equal to Solution Provided Date when there is no Temporary Solution.</errorMessage>
</ValidationRule>
