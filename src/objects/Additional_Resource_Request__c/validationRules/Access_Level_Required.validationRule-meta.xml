<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Access_Level_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL( Additional_Resource__c , &quot;Library&quot;), ISPICKVAL( Access_Level__c , &quot;&quot;))</errorConditionFormula>
    <errorDisplayField>Access_Level__c</errorDisplayField>
    <errorMessage>Please enter Access Level</errorMessage>
</ValidationRule>
