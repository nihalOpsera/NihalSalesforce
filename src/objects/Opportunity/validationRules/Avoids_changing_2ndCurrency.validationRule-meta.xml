<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Avoids_changing_2ndCurrency</fullName>
    <active>true</active>
    <errorConditionFormula>ISCHANGED( X2nd_Currency__c ) &amp;&amp; NOT(ISBLANK( G5_Contract_Approval_Date__c)) &amp;&amp; ($Profile.Name&lt;&gt;$Label.Data_Loader_Profile_Name)</errorConditionFormula>
    <errorDisplayField>X2nd_Currency__c</errorDisplayField>
    <errorMessage>Can&apos;t Change the 2nd Currency after G5 Approval date is Filled.</errorMessage>
</ValidationRule>
