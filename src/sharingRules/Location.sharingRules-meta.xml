    <sharingCriteriaRules>
        <fullName>CH_SEC_ALTICE</fullName>
        <accessLevel>Read</accessLevel>
        <description>NOKIASC-24740: Share Locations with SEC|ALTICE||| when Customer Compliance = US_ALTICE.</description>
        <label>SEC|ALTICE|||</label>
        <sharedTo>
            <group>SEC_ALTICE</group>
        </sharedTo>
        <criteriaItems>
            <field>CH_AccountCustomerCompliance__c</field>
            <operation>equals</operation>
            <value>US_ALTICE</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
