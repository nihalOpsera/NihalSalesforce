<?xml version="1.0" encoding="UTF-8"?>
<LightningMessageChannel xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Used in the GRAX application to pass version ids between components</description>
    <isExposed>true</isExposed>
    <lightningMessageFields>
        <description>The Id of the version selected</description>
        <fieldName>versionId</fieldName>
    </lightningMessageFields>
    <lightningMessageFields>
        <description>True when checked false when unchecked</description>
        <fieldName>checked</fieldName>
    </lightningMessageFields>
    <lightningMessageFields>
        <description>The Id of the version selected</description>
        <fieldName>addVersionId</fieldName>
    </lightningMessageFields>
    <lightningMessageFields>
        <description>True when checked false when unchecked</description>
        <fieldName>addVersionChecked</fieldName>
    </lightningMessageFields>
    <lightningMessageFields>
        <description>initialized or resolved</description>
        <fieldName>state</fieldName>
    </lightningMessageFields>
    <masterLabel>versionSelected</masterLabel>
</LightningMessageChannel>
