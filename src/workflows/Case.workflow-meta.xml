    <fieldUpdates>
        <fullName>CH_ConcatenateValues</fullName>
        <field>Description</field>
        <formula>IF(CH_RecordTypeDevName__c =&apos;CH_Problem&apos;, LEFT(CH_ProblemStatement__c + &apos; &apos; + CH_Product_Name__c + &apos; &apos; + CH_Product_Release__r.Name + &apos; &apos; + CH_SW_Component__c + &apos; &apos; + CH_SW_Release_Name__c , 32000), LEFT(CH_Product_Name__c + &apos; &apos; + CH_Product_Release_Name__c + &apos; &apos; + CH_CustomerDescription__c + &apos; &apos; + CH_IssueDescription__c + &apos; &apos; + CH_InitialDiagnosis__c + &apos; &apos; + CH_TemporarySolution__c , 32000))</formula>
