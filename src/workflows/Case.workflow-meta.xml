    <rules>
        <fullName>CH Status Update to Cancelled for Problem Cases</fullName>
        <actions>
            <name>Update_Stage_to_Closed</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Status_to_Cancelled</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>NOKIASC-34633 Workflow to set the status of a Problem case automatically to Cancelled and stage to Closed, when the Closure Reason is set to Cancelled.</description>
        <formula>AND(ISCHANGED( CH_Closure_Reason__c  ) ,RecordType.DeveloperName  = &apos;CH_Problem&apos;, ISPICKVAL(CH_Closure_Reason__c, &apos;Cancelled&apos;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
