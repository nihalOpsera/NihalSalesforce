        </criteriaItems>
        <criteriaItems>
            <field>Lead.Status</field>
            <operation>equals</operation>
            <value>Open</value>
        </criteriaItems>
        <description>Req #2496</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
