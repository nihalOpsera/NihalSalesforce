                                                <a aura:id="ViewProductsDirect" onclick="{!c.viewProductsDirect}">
                                                    <img src="/resource/1536158833000/Nokia_CPQ_Cart" alt="View Products" border="0" title="View Products"/>
                                                </a>
                                                <br/>
