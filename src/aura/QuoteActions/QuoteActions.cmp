    <aura:attribute name="proposal" type="Apttus_Proposal__Proposal__c"/>
    <aura:attribute name="isPricingManager" type="Boolean" default="false"/>
    <aura:attribute name="isSubmitApproval" type="Boolean" default="true"/>
