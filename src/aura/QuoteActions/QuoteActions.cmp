                        			<aura:if isTrue="{!and(v.proposal.Turbo_Pricing_Enable__c==true,v.proposal.Quote_Type__c=='Direct CPQ',v.proposal.NokiaCPQ_Portfolio__c=='IP Routing')}"> 
